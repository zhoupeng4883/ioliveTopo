<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             alpha.disabled="0.5">
     
    <fx:Metadata>
        <![CDATA[ 
        /** 
         * @copy spark.skins.spark.ApplicationSkin#hostComponent
         */
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
	</s:states>	
  <!--<s:BitmapImage source="{hostComponent.iconDisplay.bitmapData}"/>-->
	<s:BitmapImage id="iconDisplay"/>
		<s:Group width="100%" height="100%" id="maskGroup">
		<s:Rect id="over_fill" left="-1" right="-1" top="-1" bottom="-1">
			<s:fill>
				<s:SolidColor color="0xFFFFFF" alpha="0" alpha.over="0.8"/>
			</s:fill>
		</s:Rect>
	</s:Group>
</s:SparkButtonSkin>
